
@{
    ViewData["Title"] = "CreatedPerson";

}

<section>

    <h1>New Business made</h1>
    @foreach (var group in Model)
    {
<section>

    <h1>The random generated business is:  </h1>

    <h4>Business Name: @group.BusinessName</h4>
    <h4>BusinessType: @group.BusinessType</h4>

</section>
    @foreach (var inventory in group.InventoryList)
    {
        <section>

           <h4>Stock name: @inventory.InventoryList[inventory]</h4>

        </section>
    }



    
    }
</section>
